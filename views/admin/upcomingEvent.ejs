


<%- include('./head') %>

		<!--page-wrapper-->
		<div class="page-wrapper">
			<!--page-content-wrapper-->
			<div class="page-content-wrapper">
				<div class="page-content">
					
					<div class="card radius-15">
						<div class="card-header border-bottom-0">
							<div class="d-flex align-items-center">
								<div>
									<h5 class="mb-0">Upcoming Events Details</h5>
								</div>
								<div class="ml-auto">
									<a href="/admin/addUpcomingEvent">
										<button type="button" class="btn btn-light radius-15">
											Add More Upcoming Events
										</button>
									</a>
								</div>
							</div>
						</div>
						<div class="card-body p-0">
							<div class="table-responsive">
								<table class="table mb-0">
									<thead>
										<tr>
											<th>SI.No</th>
											<th>Event Hedding</th>
											<th>Event Type </th>
											<th>Event Date</th>
											<th>Location</th>
											<th>Active</th>
											<th>Edit</th>
										</tr>
									</thead>



									
									<tbody>
										<% if (events && events.length > 0) { %>
											<% events.forEach((event, index) => { %>
												<tr>
													<td><%= index + 1 %></td>
													<td><%= event.eventHeading %></td>
													<td><%= event.eventType %></td>
													<td><%= new Date(event.eventDate).toLocaleDateString() %></td>
													<td><%= event.location %></td>
													<td>
														<a href="/admin/editUpcomingEventStatus/<%= event._id %>?status=<%= event.status %>" 
														   class="btn btn-sm btn-light btn-block radius-30">
															<%= event.status ? 'Active' : 'Inactive' %>
														</a>
													</td>
													<td>
														<a href="/admin/editUpcomingEvent/<%= event._id %>" class="btn btn-sm btn-light btn-block radius-30">
															Edit
														</a>
													</td>
												</tr>
											<% }); %>
										<% } else { %>
											<tr>
												<td colspan="7" class="text-center">No Upcoming Events Found</td>
											</tr>
										<% } %>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!--end page-content-wrapper-->
		</div>
		<!--end page-wrapper-->
		<!--start overlay-->
		<div class="overlay toggle-btn-mobile"></div>
		<!--end overlay-->
		<!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
		<!--End Back To Top Button-->
