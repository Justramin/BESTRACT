                <!-- contact form -->
                <section id="contact">
                    <div class="container mil-p-120-90">
                        <h3 class="mil-center mil-up mil-mb-120">Find<span class="mil-thin">Job</span></h3>
                        <form action="/getHiredPost" method="post" class="row align-items-center">
                            <div class="col-lg-6 mil-up">
                                <input type="text" name="name" placeholder="What's your name" required>
                            </div>
                            <div class="col-lg-6 mil-up">
                                <input type="number" name="phone" placeholder="Your Phone Number" required>
                            </div>
                            <div class="col-lg-6 mil-up">
                                <input type="text" name="interests" placeholder="What kind of job search are you..." required>
                            </div>
                            <div class="col-lg-6 ">
                                <input type="email" name="email" placeholder="Your Email" required>
                            </div>
                            <div class="col-lg-12 mil-up">
                                <textarea name="message" placeholder="Tell us about your job search interests or the positions you're considering." required></textarea>
                            </div>
                            <div class="col-lg-8">
                                <p class="mil-up mil-mb-30">
                                    <span class="mil-accent">*</span> We promise not to disclose your personal information to third parties.
                                </p>
                            </div>
                            <div class="col-lg-4">
                                <div class="mil-adaptive-right mil-up mil-mb-30">
                                    <button type="submit" class="mil-button mil-arrow-place">
                                        <span>Send message</span>
                                    </button>
                                </div>
                            </div>
                        </form>
                        
                    </div>
                </section>
